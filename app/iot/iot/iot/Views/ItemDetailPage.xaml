<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="iot.Views.ItemDetailPage"
             Title="{Binding Title}">

    <StackLayout Spacing="20" Padding="15">
        <Label Text="Location:" FontSize="Medium" />
        <Label Text="{Binding Location}" FontSize="Small"/>
        <Label Text="Occupied:" FontSize="Medium" />
        <Label Text="{Binding Occupied}" FontSize="Small"/>
        <Label Text="Spot:" FontSize="Medium" />
        <Label Text="{Binding Spot}" FontSize="Small"/>
    </StackLayout>
    
</ContentPage>